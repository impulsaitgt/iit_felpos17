       t-inherit-mode="extension" owl="1">
        <xpath expr="//div[@class='row row-cols-1 row-cols-sm-2 gy-3 mt-3']" position="inside">
            <div class="client-detail">
                <span class="label">Nombre Factura</span>
                <input class="detail nombre sat" name="fel_nombre_sat" t-on-change="captureChange"
                       t-att-value="props.partner.fel_nombre_sat || ''" t-att-readonly="false"/>
            </div>

        </xpath>
        </t>
</template>